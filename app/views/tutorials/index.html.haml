.span-24.last
  = render 'shared/notice'
.span-24.contentBox.last
  %h3.span-10 Tutorials 
  .span-5.right
    .right= link_to 'Add', new_tutorial_path
  %table
    - @tutorials.each do |tutorial|
      %tr
        %td= tutorial.to_s
        %td= tutorial.title
        %td= link_to 'Edit', edit_tutorial_path(tutorial), :id => "editTutorial_#{tutorial.id}"
        %td= link_to 'Delete', tutorial_path(tutorial), :id => "deleteTutorial_#{tutorial.id}"
