= render 'shared/notice'
%h3 Nodes
%table
  - @nodes.each do |node|
    %tr
      %td= link_to node.title, node
      %td= link_to 'Edit', edit_node_path(node), :id => "editNode_#{node.id}"
      %td= link_to 'Destroy', node, :confirm => "Are you sure?", :method => :delete, :id => "deleteNode_#{node.id}"

